<UserControl x:Class="RelertSharp.Wpf.MapEngine.MainPanel"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:RelertSharp.Wpf.MapEngine"
             xmlns:d3d="clr-namespace:System.Windows.Interop;assembly=PresentationCore"
             mc:Ignorable="d" Loaded="PanelLoaded">
    <UserControl.Resources>
        <ImageBrush x:Key="SceneResource"/>
    </UserControl.Resources>
    <Grid x:Name="gridMain" MouseMove="HandleMouseMove" MouseWheel="HandleMouseWheel" SizeChanged="HandleSizeChanged" MouseDown="HandleMouseDown" MouseUp="HandleMouseUp">
        <Canvas x:Name="graphicTop" Panel.ZIndex="2"/>
        <Image x:Name="imgelt" Stretch="Fill" Panel.ZIndex="1">
            <Image.Source>
                <d3d:D3DImage x:Name="d3dimg"/>
            </Image.Source>
        </Image>
    </Grid>
</UserControl>
