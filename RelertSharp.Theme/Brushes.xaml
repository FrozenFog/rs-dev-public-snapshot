<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!--Main-->
    <SolidColorBrush x:Key="textFront" Color="#FFF1F1F1"/>
    <SolidColorBrush x:Key="dimBack" Color="#FF2E2E2E"/>
    <SolidColorBrush x:Key="textDisable" Color="#FF7D7D7D"/>
    <SolidColorBrush x:Key="dimDisable" Color="#FF222222"/>

    <!--Buttons-->
    <SolidColorBrush x:Key="btnBack" Color="#FF464646"/>
    <SolidColorBrush x:Key="btnBorder" Color="#FF555555"/>
    <SolidColorBrush x:Key="btnHighLight" Color="#FF565656"/>

    <!--Textboxes-->
    <SolidColorBrush x:Key="txbBorder" Color="#FF555555"/>
    <SolidColorBrush x:Key="txbBack" Color="#FF2E2E2E"/>
    <SolidColorBrush x:Key="txbDisable" Color="#FF525252"/>
    
    <!--ScrollBar-->
    <SolidColorBrush x:Key="scrLight" Color="#686868"/>
    <SolidColorBrush x:Key="scrBack" Color="#3E3E3E"/>
    <SolidColorBrush x:Key="scrMark" Color="#999999"/>
    
    <!--Menu-->
    <SolidColorBrush x:Key="menuDimBack" Color="#1b1b1b"/>
    <SolidColorBrush x:Key="menuBack" Color="#202020"/>
    <SolidColorBrush x:Key="menuSep" Color="#373737"/>
    <SolidColorBrush x:Key="menuHigh" Color="#404040"/>
    
    <!--Selected-->
    <SolidColorBrush x:Key="selectedBlue" Color="#0078D7"/>
    <SolidColorBrush x:Key="disableBlue" Color="#00457c"/>
</ResourceDictionary>